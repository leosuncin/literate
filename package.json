{
  "name": "literate",
  "version": "0.0.0",
  "private": true,
  "description": "A blog build with React",
  "license": "GPL-3.0",
  "author": "Jaime Leonardo Suncin Cruz <leosuncin@gmail.com>",
  "main": "next.config.js",
  "scripts": {
    "build": "next build",
    "cy:open": "cypress open",
    "debug": "NODE_OPTIONS='--inspect-brk' next",
    "dev": "next dev",
    "export": "next export",
    "fixtures:load": "mongodb-fixtures load -p cypress/fixtures -u $MONGODB_URL",
    "fixtures:rebuild": "mongodb-fixtures rebuild -p cypress/fixtures -u $MONGODB_URL",
    "fixtures:unload": "mongodb-fixtures unload -p cypress/fixtures -u $MONGODB_URL",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.tsx .",
    "lint:tsc": "tsc --noEmit --noErrorTruncation --pretty false --incremental false",
    "postinstall": "patch-package",
    "start": "next start",
    "start:e2e": "start-test dev 3000 test:e2e",
    "test": "jest",
    "test:e2e": "cypress run",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "React",
    "blog"
  ],
  "dependencies": {
    "@sindresorhus/slugify": "^1.1.0",
    "@xstate/react": "^1.0.1",
    "cross-fetch": "^3.0.6",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.25.0",
    "mongoose": "^5.10.10",
    "mongoose-id-validator": "^0.6.0",
    "mongoose-unique-validator": "^2.0.3",
    "next": "^9.5.5",
    "ololog": "^1.1.162",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^6.9.6",
    "swr": "^0.3.5",
    "xstate": "^4.13.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@bahmutov/cy-api": "^1.4.2",
    "@cypress/code-coverage": "^3.8.2",
    "@cypress/schema-tools": "^4.7.5",
    "@cypress/webpack-preprocessor": "^5.4.8",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@shelf/jest-mongodb": "^1.2.3",
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.8",
    "@types/faker": "^5.1.2",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/node": "^14.11.10",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/yup": "^0.29.8",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-typescript-to-proptypes": "^1.4.1",
    "cypress": "^5.4.0",
    "cypress-dotenv": "^1.2.2",
    "cypress-wait-until": "^1.7.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-deprecation": "^1.1.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jest-formatting": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-testing-library": "^3.9.2",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-wes": "^3.0.2",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^26.5.3",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.4.2",
    "node-mocks-http": "^1.9.0",
    "node-mongodb-fixtures": "^3.2.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "patch-package": "^6.2.2",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "0.0.6",
    "prettier-plugin-package": "^1.2.0",
    "source-map-support": "^0.5.19",
    "start-server-and-test": "^1.11.5",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "report-dir": "coverage/e2e",
    "exclude": [
      "pages/api/__coverage__.ts"
    ]
  }
}
