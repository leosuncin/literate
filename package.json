{
  "name": "literate",
  "version": "0.0.0",
  "private": true,
  "description": "A blog build with React",
  "license": "GPL-3.0",
  "author": "Jaime Leonardo Suncin Cruz <leosuncin@gmail.com>",
  "main": "next.config.js",
  "scripts": {
    "build": "next build",
    "cy:open": "cypress open",
    "debug": "NODE_OPTIONS='--inspect-brk' next",
    "dev": "next dev",
    "export": "next export",
    "fixtures:load": "mongodb-fixtures load -u $MONGODB_URL",
    "fixtures:rebuild": "mongodb-fixtures rebuild -u $MONGODB_URL",
    "fixtures:unload": "mongodb-fixtures unload -u $MONGODB_URL",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.tsx .",
    "lint:tsc": "tsc --noEmit --noErrorTruncation --pretty false --incremental false",
    "prepare": "patch-package; husky install",
    "start": "next start",
    "start:e2e": "start-test dev 3000 test:e2e",
    "test": "jest",
    "test:e2e": "cypress run",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "React",
    "blog"
  ],
  "dependencies": {
    "@sindresorhus/slugify": "^2.1.0",
    "@xstate/react": "^1.3.2",
    "cross-fetch": "^3.1.4",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.26.0",
    "mongoose": "^5.12.3",
    "mongoose-id-validator": "^0.6.0",
    "mongoose-unique-validator": "^2.0.3",
    "next": "^11.0.1",
    "ololog": "^1.1.175",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.10.0",
    "swr": "^0.5.5",
    "xstate": "^4.17.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@bahmutov/cy-api": "^1.5.0",
    "@cypress/code-coverage": "^3.9.4",
    "@cypress/schema-tools": "^4.7.7",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@shelf/jest-mongodb": "^1.2.4",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.5",
    "@types/faker": "^5.5.3",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.4",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/node": "^16.0.0",
    "@types/node-mongodb-fixtures": "^3.2.0",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-typescript-to-proptypes": "^1.4.2",
    "cypress": "^7.2.0",
    "cypress-dotenv": "^1.2.2",
    "cypress-wait-until": "^1.7.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.25.0",
    "eslint-config-next": "^11.0.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-deprecation": "^1.2.0",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-testing-library": "^4.1.2",
    "faker": "^5.5.3",
    "husky": "^7.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-wes": "^3.0.2",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^27.0.6",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^11.0.0",
    "node-mocks-http": "^1.10.1",
    "node-mongodb-fixtures": "^3.2.8",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.2.1",
    "prettier-plugin-import-sort": "0.0.7",
    "prettier-plugin-package": "^1.3.0",
    "source-map-support": "^0.5.19",
    "start-server-and-test": "^1.12.1",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.1.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "report-dir": "coverage/e2e",
    "exclude": [
      "pages/api/__coverage__.ts"
    ]
  }
}
