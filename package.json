{
  "name": "literate",
  "version": "0.0.0",
  "private": true,
  "description": "A blog build with React",
  "license": "GPL-3.0",
  "author": "Jaime Leonardo Suncin Cruz <leosuncin@gmail.com>",
  "main": "next.config.js",
  "scripts": {
    "build": "next build",
    "cy:open": "cypress open",
    "debug": "NODE_OPTIONS='--inspect-brk' next",
    "dev": "next dev",
    "export": "next export",
    "fixtures:load": "mongodb-fixtures load -p cypress/fixtures -u $MONGODB_URL",
    "fixtures:rebuild": "mongodb-fixtures rebuild -p cypress/fixtures -u $MONGODB_URL",
    "fixtures:unload": "mongodb-fixtures unload -p cypress/fixtures -u $MONGODB_URL",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.tsx .",
    "lint:tsc": "tsc --noEmit --noErrorTruncation --incremental false",
    "postinstall": "patch-package",
    "start": "next start",
    "start:e2e": "start-test dev 3000 test:e2e",
    "test": "jest",
    "test:e2e": "cypress run",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "React",
    "blog"
  ],
  "dependencies": {
    "@sindresorhus/slugify": "^1.1.0",
    "@xstate/react": "^0.8.1",
    "cross-fetch": "^3.0.5",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.24.1",
    "mongoose": "^5.9.28",
    "mongoose-id-validator": "^0.6.0",
    "mongoose-unique-validator": "^2.0.3",
    "next": "^9.5.2",
    "ololog": "^1.1.161",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.4.0",
    "swr": "^0.3.0",
    "xstate": "^4.11.0",
    "yup": "^0.29.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.1",
    "@cypress/webpack-preprocessor": "^5.4.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@shelf/jest-mongodb": "^1.2.2",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/yup": "^0.29.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-typescript-to-proptypes": "^1.4.0",
    "cypress": "^4.12.1",
    "cypress-wait-until": "^1.7.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.8",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-wes": "^3.0.2",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^25.5.4",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.2.11",
    "node-mocks-http": "^1.8.1",
    "node-mongodb-fixtures": "^3.2.5",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "patch-package": "^6.2.2",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "0.0.4",
    "prettier-plugin-package": "^1.0.0",
    "source-map-support": "^0.5.19",
    "start-server-and-test": "^1.11.3",
    "ts-jest": "^25.5.1",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "report-dir": "coverage/e2e",
    "exclude": [
      "pages/api/__coverage__.ts"
    ]
  }
}
